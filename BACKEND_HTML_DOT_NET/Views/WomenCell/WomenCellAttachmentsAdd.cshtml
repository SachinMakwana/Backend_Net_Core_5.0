@{ Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "WomenCellAttachmentsAdd"; }

<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0 text-dark">Add Women Cell Attachments Details</h1>
                </div>
                <!-- /.col -->
                <div class="col-sm-6">
                    <a href="@Url.Action("WomenCellAttachmentsView","WomenCell")">
                        <ol class="breadcrumb float-sm-right">
                            <li><i class="nav-icon fas fa-minus-circle"></i></li>
                            <li>Back To Details</li>
                        </ol>
                    </a>
                </div>
                <!-- /.col -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <div class="row ">
                <div class="col-12">
                    <!--ur contents here-->
                    <!-- jquery validation -->
                    <div class="card card-primary">
                        <!-- /.card-header -->
                        <!-- form start -->
                        <form role="form" id="quickForm">
                            <div class="card-body">
                                <div class="input-group">
                                    <div class="col-6">
                                        <label for="Name"> Enter Title</label>
                                        <input type="text" class="form-control" id="Name"
                                               placeholder="Enter Title">
                                    </div>
                                    <div class="col-6">
                                        <label for="exampleInputFile">Upload Attachment File</label>
                                        <div class="input-group">
                                            <div class="custom-file">
                                                <input type="file" class="custom-file-input"
                                                       id="exampleInputFile">
                                                <label class="custom-file-label" for="exampleInputFile">
                                                    Choose
                                                    file
                                                </label>
                                            </div>
                                            <div class="input-group-append">
                                                <span class="input-group-text" id="">Upload</span>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </form>

                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-6">
                    <button type="submit" class="btn btn-primary float-right">Submit</button>
                </div>
                <div class="col-6">
                    <a href="@Url.Action("WomenCellAttachmentsView","WomenCell")" class="btn btn-default">Clear</a>
                </div>
            </div>
        </div>
    </section>
</div>